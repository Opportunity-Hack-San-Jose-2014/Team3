<script>
    $(function () {
        $('.btn-radio').click(function(e) {
            $('.btn-radio').not(this)
                    .siblings('input').prop('checked',false)
                    .siblings('.btn-radio').removeClass('btn-success');
            $(this)
                    .siblings('input').prop('checked',true)
                    .siblings('.btn-radio').addClass('btn-success');
        });
    });
</script>

