                                                                <%for (var j=0; j<mentors[0][i].skills.length;j++) {
                                                                if(j <= 5) {%>
                                                                <span class="tags"><%=mentors[0][i].skills[j]%></span>
                                                                <% } } %>
                                                            </p>
                                                            <button type="button" class="btn btn-xs btn-primary btn-radio active">Select</button>
                                                            <input type="checkbox" id="left-item" class="hidden">
                                                        </div>
                                                    </div>
                                                    <%}%>
